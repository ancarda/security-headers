{
    "name": "ancarda/security-headers",
    "description": "Helper classes for common HTTP security headers",
    "keywords": [
        "http",
        "security",
        "headers",
        "csp",
        "content-security-policy"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Mark Dain",
            "email": "mark@markdain.net",
            "homepage": "https://markdain.net",
            "role": "Developer"
        }
    ],
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "platform": {
            "php": "7.0"
        }
    },
    "require": {
        "php": ">=7.0"
    },
    "require-dev": {
        "php-coveralls/php-coveralls": "^2.0",
        "phploc/phploc": "^4.0",
        "phpstan/phpstan-phpunit": "^0.9.4",
        "phpstan/phpstan-strict-rules": "^0.9.0",
        "phpstan/phpstan": "^0.9.2",
        "phpunit/phpunit": "^6.5",
        "squizlabs/php_codesniffer": "^3.2"
    },
    "autoload": {
        "psr-4": {
            "Ancarda\\Security\\Header\\": "src"
        }
    }
}
